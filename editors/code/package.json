{
  "name": "asyncflows-lsp",
  "description": "asyncflows-lsp",
  "license": "MIT",
  "version": "0.1.1",
  "repository": {
    "url": "https://github.com/asynchronous-flows/asyncflows-lsp/tree/yaml-language-server"
  },
  "publisher": "urosmrkobrada",
  "categories": [
    "Linters",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "multi-root ready",
    "jinja",
    "minijinja",
    "rust",
    "asyncflows-lsp",
    "python"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "dist/extension",
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.yaml"
        ],
        "scopeName": "textblock.asyncflows-lsp",
        "path": "./syntaxes/asyncflows-lsp.json"
      }
    ]
  },
  "scripts": {
    "vscode:prebulish": "webpack --mode production",
    "esbuild-base": "esbuild ./client/src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -b",
    "compile3": "webpack --mode none",
    "watch": "tsc -b -w",
    "lint": "eslint ./client/src --ext .ts,.tsx",
    "test": "sh ./scripts/e2e.sh",
    "test2": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish",
    "rebuild": "electron-rebuild"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@types/node": "^18.14.6",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/vsce": "^2.26.1",
    "bindings": "^1.5.0",
    "electron": "^30.0.8",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "node-loader": "^2.0.0"
  },
  "dependencies": {
    "@tree-sitter-grammars/tree-sitter-yaml": "^0.6.1",
    "asyncflows-lsp": "^0.1.2-alpha.1",
    "jschardet": "^3.1.2",
    "tree-sitter": "^0.21.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "umd-compat-loader": "^2.1.2",
    "vscode-languageclient": "^9.0.1",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0",
    "yaml-language-server": "^1.14.0"
  },
  "extensionDependencies": [
    "ms-python.python"
  ]
}
