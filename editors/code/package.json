{
  "name": "asyncflows-lsp",
  "description": "asyncflows-lsp",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/asynchronous-flows/asyncflows-lsp/tree/yaml-language-server"
  },
  "publisher": "urosmrkobrada",
  "categories": [
    "Linters",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "multi-root ready",
    "jinja",
    "minijinja",
    "rust",
    "asyncflows-lsp",
    "python"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [
    "onLanguage:python",
    "onLanguage:yaml"
  ],
  "main": "dist/extension",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./client/src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -b",
    "compile2": "npm run vscode:prepublish && npm run compile",
    "watch": "tsc -b -w",
    "lint": "eslint ./client/src --ext .ts,.tsx",
    "test": "sh ./scripts/e2e.sh",
    "test2": "vscode-test",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "2.21.1",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "mocha": "^10.3.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
